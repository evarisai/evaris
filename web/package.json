{
	"name": "evaris-web-app",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"postinstall": "prisma generate --schema=../shared/prisma/schema.prisma --generator client_js",
		"dev": "vite",
		"dev:backend": "pnpm --filter @evarisai/backend dev",
		"dev:worker": "pnpm --filter @evarisai/backend worker:dev",
		"dev:server": "cd ../server && uv run uvicorn evaris_server.app:app --reload --port 8080",
		"dev:all": "concurrently \"pnpm dev\" \"pnpm dev:backend\" \"pnpm dev:worker\" \"pnpm dev:server\"",
		"build": "npx prisma generate --schema=../shared/prisma/schema.prisma --generator client_js && vite build",
		"build:backend": "pnpm --filter @evarisai/backend build",
		"build:all": "pnpm build && pnpm build:backend",
		"start": "node .output/server/index.mjs",
		"start:backend": "pnpm --filter @evarisai/backend start",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"db:generate": "prisma generate --schema=../shared/prisma/schema.prisma",
		"db:push": "prisma db push --schema=../shared/prisma/schema.prisma",
		"db:migrate": "prisma migrate dev --schema=../shared/prisma/schema.prisma",
		"db:studio": "prisma studio --schema=../shared/prisma/schema.prisma",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
