# Evaris SDK Configuration
# Copy this file to .env and fill in your values

# Required: Your Evaris API key (get from dashboard)
EVARIS_API_KEY=ev_your_api_key_here

# Optional: Base URL (defaults to production)
# For local development, use:
EVARIS_BASE_URL=http://localhost:4000

# Optional: Default project ID
# If not set, you must pass project_id to each method call
EVARIS_PROJECT_ID=your_project_id
